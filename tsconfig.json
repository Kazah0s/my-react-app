{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    // "allowImportingTsExtensions": true,
  },
  "include": ["src", "babel.config.ts"],
  "exclude": ["node_modules"]
}